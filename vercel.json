{
  "redirects": [
    {
      "source": "/",
      "destination": "/dashboard",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "cache-control",
          "value": "s-maxage=300, stale-while-revalidate=60"
        }
      ]
    }
  ]
}
